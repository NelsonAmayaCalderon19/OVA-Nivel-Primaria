<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Certificado</servlet-name>
        <servlet-class>control.Certificado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Certificado_Secundaria</servlet-name>
        <servlet-class>control.Certificado_Secundaria</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Colegio</servlet-name>
        <servlet-class>control.Colegio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Control_Rector</servlet-name>
        <servlet-class>control.Control_Rector</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Control_Docente</servlet-name>
        <servlet-class>control.Control_Docente</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Certificado</servlet-name>
        <url-pattern>/Certificado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Certificado_Secundaria</servlet-name>
        <url-pattern>/Certificado_Secundaria</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Colegio</servlet-name>
        <url-pattern>/Colegio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Control_Rector</servlet-name>
        <url-pattern>/Control_Rector</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Control_Docente</servlet-name>
        <url-pattern>/Control_Docente</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
